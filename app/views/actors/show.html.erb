<div class="page-header">
  <h1>Actor #<%= @actor.id %></h1>
</div>

<p><a href="/actors" class="btn btn-primary">Go Back</a></p>

<dl class="dl-horizontal">
  <dt>Dob</dt>
  <dd><%= @actor.dob %></dd>

  <dt>Name</dt>
  <dd><%= @actor.name %></dd>

  <dt>Bio</dt>
  <dd><%= @actor.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @actor.image_url %>" width="320"></dd>

</dl>
